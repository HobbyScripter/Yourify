{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "arcanedev/no-captcha": "^4.0",
        "artem-schander/l5-modular": "^1.4",
        "barryvdh/laravel-debugbar": "^2.4",
        "barryvdh/laravel-ide-helper": "^2.3",
        "cocur/slugify": "^2.4",
        "creativeorange/gravatar": "~1.0",
        "cviebrock/eloquent-sluggable": "^4.2",
        "doctrine/dbal": "^2.5",
        "filp/whoops": "^2.1",
        "igaster/laravel-theme": "^1.14",
        "kepex/laravel-crud-generator": "^0.1.7",
        "kim/activity": "^1.1",
        "kris/laravel-form-builder": "^1.11",
        "laravel/dusk": "^1.0",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "mcamara/laravel-localization": "^1.2",
        "rachidlaasri/laravel-installer": "^1.5",
        "rcrowe/twigbridge": "^0.9.4",
        "themsaid/laravel-langman": "^1.3",
        "zarlach/laravel-twitch-api": "^1.3",
        "zizaco/entrust": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database",
            "core/Support"
        ],
        "files":[
          "core/Support/helpers.php"
        ],
        "psr-4": {
            "Yourify\\": "core/",
            "Modules\\": "core/modules",
            "Themes\\": "theme/",
            "Core\\": "core/",
            "Darkevolution\\Theme\\": "packages/darkevolution/theming/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
